                                </div>
                                <div class="form-group col-md-2">

                                    <input type="number" maxlength="2" minlength="2" min="01" max="90" class="form-control input-sm" value="@Model.TwoSureValueB" id="TwoSureValueB" name="TwoSureValueB" required="" data-parsley-required-message="Btw (01 & 90)">
                                </div>
                            </div>

                            THREE DIRECT
