                    @Html.Hidden("EChartId", Model.EChartId)
                    @Html.Hidden("GameId", Model.GameId)
                    @Html.Hidden("GameTypeId", Model.GameTypeId)
                    @Html.Hidden("OperatorId", Model.OperatorId)
                    @Html.Hidden("GameScheduleId", Model.GameScheduleId)
