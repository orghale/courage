<script src="~/Scripts/main/bootstrap-datepicker.js"></script>
    <script>

        $(document).ready(function () {

             $('.chartDatePicker').datepicker({
                startDate: '-0d',
                endDate: '+7d',
                yearRange: 'c-10:c',
                calendarWeeks: false,
                todayHighlight: true,
                clearBtn: true,
                autoclose: true
           
             });
        });

        //-----------------------Banker Value-----------------------
        $('#BankerValue').change(function () {
            if (!($('#BankerValue').val() == null ||
                $('#BankerValue').val() === "")) {

                $('#TwoSureValueA').val("");
                $('#ThreeDirectValueA').val("");
                var banker = $('#BankerValue').val();

                $('#TwoSureValueA').val(banker);
                $('#ThreeDirectValueA').val(banker);

            }
        });
        //-----------------------End Banker Value---------------------

        //-----------------------Two sure-----------------------
        $('#TwoSureValueB').change(function () {
            if ($('#TwoSureValueA').val() === $('#TwoSureValueB').val()) {

                alert("You may not repeat the same number twice for TWO-SURE Values");
                $(this).val("");
                $(this).select();
                return false;
            }

            if (!($('#TwoSureValueB').val() == null ||
                $('#TwoSureValueB').val() === "")) {

                $('#ThreeDirectValueB').val("");
                var twoSure = $('#TwoSureValueB').val();

                $('#ThreeDirectValueB').val(twoSure);

            }
        });
        //-----------------------End Two sure---------------------

        //-----------------THREE DIRECT------------------------
        $('#ThreeDirectValueB').change(function () {
            if (!($('#ThreeDirectValueB').val() == null ||
                $('#ThreeDirectValueB').val() === "")) {
