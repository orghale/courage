                    <div class="row">

                        <div class="col-md-6">

                            <div class="row">

                                <div class="col-md-6">
                                    DATE
                                    <div class="form-group">
                                        <div class="input-group date chartDatePicker" id='ChartDate' data-date-format="dd/mm/yyyy">
                                            @Html.TextBoxFor(model => model.ChartDate, new { @class = "form-control" })
                                            <span class="input-group-addon">
                                                <span class="fa fa-calendar"></span>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    GAME
                                    <div class="form-group">
                                        <input type="text" class="form-control" readonly value="@Model.GameName" id="GameName" name="GameName" required="" data-parsley-required-message="Please check Game name">
                                    </div>
                                </div>


                            </div>

                            TITLE
                            <div class="row">
                                <div class="form-group col-md-12">
                                    <input type="text" class="form-control" value="@Model.Title" id="Title" name="Title">
                                </div>
                            </div>
                            
                            BANKER
