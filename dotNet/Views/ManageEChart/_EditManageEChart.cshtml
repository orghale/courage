                                    <input type="number" maxlength="2" minlength="2" class="form-control input-sm" readonly value="@Model.ThreeDirectValueA" id="ThreeDirectValueA" name="ThreeDirectValueA" required="" data-parsley-required-message="Btw (01 & 90)">
                                </div>
                                <div class="form-group col-md-2">

                                    <input type="number" maxlength="2" minlength="2" class="form-control input-sm" readonly value="@Model.ThreeDirectValueB" id="ThreeDirectValueB" name="ThreeDirectValueB" required="" data-parsley-required-message="Btw (01 & 90)">
                                </div>
                                <div class="form-group col-md-2">

